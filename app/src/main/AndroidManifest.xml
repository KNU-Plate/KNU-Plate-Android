<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.knuplate">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KnuPlate"
        android:usesCleartextTraffic="true">
        <activity android:name=".RestListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <!-- usesCleartextTraffic : http 접근 허용 -->
        <activity android:name=".Ex1Activity" />
        <activity android:name=".UnivcertActivity" />
        <activity android:name=".SigninActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".TitleActivity">

        </activity>
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>